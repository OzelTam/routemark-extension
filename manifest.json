{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["storage","tabs"],
    "content_scripts":[
        {
            "js":["content.js"],
            "matches":[
                "<all_urls>"
            ]
        }
    ],
    "browser_action": {
        "default_title": "Google Mail",  
        "default_popup": "pop.html"    
      },
     
    "manifest_version": 2
}